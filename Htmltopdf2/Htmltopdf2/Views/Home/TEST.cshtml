
@{
    ViewData["Title"] = "TEST";
}
<div id="content">
    <a href="javascript:Gen()">TIKLA</a>
    <a href="javascript:GenerateDiv()">TIKLA2</a>
    <a href="javascript:gen3()">TIKLA3</a>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="~/js/jspdf.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/0.4.1/html2canvas.min.js"></script>

    <div id="buDiviPdfYap">
        <p>Selam</p>
        <img src="~/images/minor major akords.jpg" />
    </div>
    <script>

        var doc = new jsPDF();
        var specialElementHandlers = {
            '#editor': function (element, renderer) {
                return true;
            }
        };


        function Gen() {
            var doc = new jsPDF();
            doc.text(20, 20, 'TEXT MESSAGE');
            doc.addPage();
            doc.text(20, 20, 'TEXT MESSAGE2');
            doc.save('Test.pdf');
        }

        function GenerateDiv() {
            var doc = new jsPDF();
            doc.fromHTML($('#buDiviPdfYap').get(0), 20, 20, { 'width': 500 });
            doc.save('test2.pdf')
        }
        function gen3() {
            html2canvas($('#buDiviPdfYap'), {
                onrendered: function (canvas) {
                    var img = canvas.toDataURL("image/png");
                    var doc = new jsPDF();
                    doc.addImage(img, 'JPEG', 20, 20);
                    doc.save('test3.pdf');
                }
            })
        }
    </script>
